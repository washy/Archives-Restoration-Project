			// }

			// OnSpawnOutput 
			// {
			// 	Target "apply_last_health"
			// 	Action "Compare"
			// 	Delay 0.06
			// }

			OnParentKilledOutput 
			{
				Target "!activator"
				Action "$SetVar$FreelanceEquipped"
				Param "0"
				Delay 0.1
			}
			
			//apply health from previous class if switched
			logic_compare
			{
				"targetname" "apply_last_health"

				"InitialValue" "-1"

